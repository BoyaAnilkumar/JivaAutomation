package Stepdefinition1;

import Pages.ViewTrimSheetscreen_Pages;
import io.cucumber.java.en.Then;

public class ViewTrimSheetscreen_Steps {
	
	ViewTrimSheetscreen_Pages VTSP = new ViewTrimSheetscreen_Pages(); 
	
	@Then ("^Navigate to View Trim Sheet screen$")
	public void Navigate_to_View_Trim_Sheet_screen() throws Throwable {
		VTSP.Navigate_to_View_Trim_Sheet_screen();
	}
	@Then ("^Verify and Print the Bulk Trim Deadline$")
	public void Verify_and_Print_the_Bulk_Trim_Deadline() throws Throwable {
		VTSP.Verify_and_Print_the_Bulk_Trim_Deadline();
	}
	@Then ("^Verify and Print the Trim Manager in View$")
	public void Verify_and_Print_the_Trim_Manager_in_View() throws Throwable {
		VTSP.Verify_and_Print_the_Trim_Manager_in_View();
	}
	@Then ("^Verify and Print the Trim Manager Assistants in View$")
	public void Verify_and_Print_the_Trim_Manager_Assistants_in_View() throws Throwable {
		VTSP.Verify_and_Print_the_Trim_Manager_Assistants_in_View();
	}
	@Then ("^Verify and Print the Merchant and Trim Team Responsibility grid records$")
	public void Verify_and_Print_the_Merchant_and_Trim_Team_Responsibility_grid_records() {
		VTSP.Verify_and_Print_the_Merchant_and_Trim_Team_Responsibility_grid_records();
	}
	

}
